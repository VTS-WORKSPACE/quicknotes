<div class='note-shares'>
	{{#each shared_with}}
		<div class="icon-user slim-share" share-id="{{ id }}" title="Shared with {{ shared_user }}">{{{ shared_user }}}</div>
	{{/each}}
	{{#each shared_by}}
		<div class="icon-user slim-share" share-id="{{ id }}" title="Shared from {{ user_id }}">{{{ user_id }}}</div>
	{{/each}}
</div>